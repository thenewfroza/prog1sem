#include <stdio.h>
#include <stdlib.h>

int main() {
    int num;
    int num1;


    printf("Vedite chislo v 16ss\n");


    scanf("%x", &num);
    printf("chislo v 8ss %o\n",num);


    printf("chislo v 16ss %x\n", num);
    int move = num << 4;
    printf("v levo na 4 bit %x\n", move);


    printf("chislo v 16ss %x\n", num);                                     //f10 3856 e20 3616
    printf("chislo v 16ss posle otr %x\n", ~num);                          //0000000000001010 << 4 = 0000000010100000

                                                                                  //a 1010 5 0101
    scanf("%x",&num1);
    printf("bit AND for num and num1 %x\n", num & num1);


    return 0;
}
